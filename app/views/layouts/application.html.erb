<!DOCTYPE html>
<html>
  <head>
    <title>Real Estates</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css' %>
    
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    

    <h3>
        <p class="navbar-text pull-right">
        <% if human_signed_in? %>
          <strong><%= current_human.email %></strong>.
          <%= link_to 'Edit profile', edit_human_registration_path, :class => 'navbar-link' %> |
          <%= link_to "Logout", destroy_human_session_path, method: :delete, :class => 'navbar-link'  %>
        <% else %>
          <%= link_to "Sign up", new_human_registration_path, :class => 'navbar-link'  %> |
          <%= link_to "Login", new_human_session_path, :class => 'navbar-link'  %>
        <% end %>
        </p>
      </h3>
  </head>

  <body>
    <%= yield %>
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
  </body>
</html>
